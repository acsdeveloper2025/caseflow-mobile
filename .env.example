# CaseFlow Mobile Environment Configuration Template
# Copy this file to .env and update the values according to your environment

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:3001
VITE_ENVIRONMENT=development

# Authentication Configuration
VITE_JWT_SECRET_KEY=your-jwt-secret-key-here
VITE_JWT_EXPIRES_IN=24h
VITE_REFRESH_TOKEN_EXPIRES_IN=7d

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable the following APIs:
# - Maps JavaScript API
# - Geocoding API
# - Places API (optional)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# App Configuration
VITE_APP_NAME=CaseFlow Mobile
VITE_APP_VERSION=2.1.0
VITE_APP_ENVIRONMENT=development

# External API Configuration
VITE_NOMINATIM_BASE_URL=https://nominatim.openstreetmap.org
VITE_NOMINATIM_USER_AGENT=CaseFlow-Mobile/2.1.0

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg,application/pdf
VITE_MAX_ATTACHMENTS_PER_CASE=10

# Location Services Configuration
VITE_LOCATION_TIMEOUT=30000
VITE_LOCATION_MAX_AGE=300000
VITE_LOCATION_HIGH_ACCURACY=true

# Offline & Sync Configuration
VITE_OFFLINE_STORAGE_LIMIT=100
VITE_SYNC_RETRY_ATTEMPTS=3
VITE_SYNC_RETRY_DELAY=5000
VITE_AUTO_SYNC_INTERVAL=300000

# Security Configuration
VITE_ENABLE_BIOMETRIC_AUTH=true
VITE_ENABLE_DEVICE_BINDING=true
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_GOOGLE_MAPS=true
VITE_ENABLE_LOCATION_VALIDATION=true
VITE_ENABLE_ENHANCED_GEOCODING=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_AUTO_SAVE=true
VITE_ENABLE_BACKGROUND_SYNC=true

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
VITE_ENABLE_MOCK_DATA=false

# Production Environment Example:
# VITE_API_BASE_URL=https://api.caseflow.com/api
# VITE_WS_URL=wss://api.caseflow.com
# VITE_ENVIRONMENT=production
# VITE_DEBUG_MODE=false
# VITE_LOG_LEVEL=error
# VITE_ENABLE_MOCK_DATA=false

# Staging Environment Example:
# VITE_API_BASE_URL=https://staging-api.caseflow.com/api
# VITE_WS_URL=wss://staging-api.caseflow.com
# VITE_ENVIRONMENT=staging
# VITE_DEBUG_MODE=true
# VITE_LOG_LEVEL=info
# VITE_ENABLE_MOCK_DATA=false
